{
  "extends": ["stylelint-config-standard"],
  "plugins": ["stylelint-scss"],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true
  },
  "ignoreFiles": [
    "build/**",
    "dist/**",
    "**/package-lock.json",
    "**/package.json",
    "**/reset*.css",
    "**/bootstrap*.css",
    "**/background.mp4"
  ]
}